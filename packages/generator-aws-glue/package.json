{
  "name": "@eventcatalog/generator-aws-glue",
  "version": "0.1.1",
  "description": "AWS Glue Schema Registry generator for EventCatalog",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "format": "prettier --write .",
    "changeset": "changeset",
    "format:diff": "prettier --list-different .",
    "release": "changeset publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eventcatalog",
    "aws",
    "glue",
    "schema-registry",
    "generator"
  ],
  "author": "EventCatalog",
  "license": "Dual License",
  "devDependencies": {
    "@smithy/types": "^3.5.0",
    "@types/node": "^20.16.1",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@aws-sdk/client-glue": "^3.658.1",
    "@changesets/cli": "^2.27.8",
    "@eventcatalog/sdk": "^2.3.0",
    "chalk": "^4",
    "update-notifier": "^7.3.1"
  }
}
